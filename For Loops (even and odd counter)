#include <iostream>
#include <vector>
  
int main(){
  int totalEven=0;
  int totalOdd=0;
std::vector<int> Bullshit = {1,2,3,4,5,6,7};
int tries =1;
for  (int i=0; i < Bullshit.size();i++){
  if (Bullshit[i]%2 == 0){
    totalEven = totalEven+ Bullshit[i];
  }
 else{
  totalOdd= totalOdd+Bullshit[i];
  }
}
std::cout << totalOdd << "\n";
std::cout << totalEven << "\n";
}
